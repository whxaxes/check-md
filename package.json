{
  "name": "check-md",
  "version": "1.0.0",
  "description": "Checks dead links of markdown",
  "bin": {
    "check-md": "./bin.js"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "commander": "^5.0.0",
    "diacritics": "^1.3.0",
    "globby": "^11.0.0"
  },
  "devDependencies": {
    "autod": "^3.1.0",
    "coffee": "^5.2.2",
    "egg-bin": "^4.14.1",
    "egg-ci": "^1.14.0",
    "egg-mock": "^4.0.0",
    "eslint": "^6.8.0",
    "eslint-config-egg": "^8.0.1",
    "js2dts": "^0.3.0",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "autod": "autod",
    "lint": "eslint .",
    "test": "npm run lint -- --fix && egg-bin pkgfiles && npm run test-local",
    "test-local": "egg-bin test",
    "gen-dts": "j2d ./index.js",
    "pub": "npm run gen-dts && npm run pkgfiles && npm publish",
    "cov": "egg-bin cov",
    "ci": "npm run lint && egg-bin pkgfiles --check && npm run cov",
    "pkgfiles": "egg-bin pkgfiles"
  },
  "ci": {
    "version": "8, 10, 11"
  },
  "eslintIgnore": [
    "coverage",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "files": [
    "index.js",
    "bin.js",
    "index.d.ts"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "author": "whxaxes <whxaxes@gmail.com>",
  "license": "MIT"
}
